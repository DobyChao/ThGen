{
  "slice": {
    "entry": "extensions/slice.py",
    "description": "Slice an image into smaller pieces.",
    "args": [
      {
        "name": "image_path",
        "type": "str",
        "help": "Path to the image image file."
      },
      {
        "name": "output_dir",
        "type": "str",
        "help": "Path to the output directory."
      },
      {
        "name": "top",
        "type": "int",
        "help": "Top coordinate of the slice"
      },
      {
        "name": "left",
        "type": "int",
        "help": "Left coordinate of the slice"
      },
      {
        "name": "bottom",
        "type": "int",
        "help": "Bottom coordinate of the slice"
      },
      {
        "name": "right",
        "type": "int",
        "help": "Right coordinate of the slice"
      },
      {
        "name": "slice_height",
        "type": "int",
        "help": "Height of the slice"
      },
      {
        "name": "slice_rows",
        "type": "int",
        "help": "Number of rows in the slice"
      },
      {
        "name": "slice_width",
        "type": "int",
        "help": "Width of the slice"
      },
      {
        "name": "slice_cols",
        "type": "int",
        "help": "Number of columns in the slice"
      }
    ]
  },
  "generate_script_json": {
    "entry": "extensions/generate_script_json.py",
    "description": "usage: generate_script_json.py [-h] [--script_path SCRIPT_PATH]",
    "args": [
      {
        "name": "script_path",
        "help": ""
      },
      {
        "name": "args_path",
        "help": ""
      }
    ]
  },
  "make_dataset": {
    "entry": "make_dataset.py",
    "description": "usage: make_dataset.py [-h] [--config CONFIG] [--num NUM]",
    "args": [
      {
        "name": "config",
        "help": "path to config file"
      },
      {
        "name": "num",
        "help": "number of images to generate"
      }
    ]
  },
  "fix_pixel": {
    "entry": "extensions/fix_pixel.py",
    "description": "usage: fix_pixel.py [-h] --input INPUT --output OUTPUT",
    "args": [
      {
        "name": "input",
        "help": "path to input image"
      },
      {
        "name": "output",
        "help": "path to output image"
      }
    ]
  },
  "multi_process": {
    "entry": "extensions/multi_process.py",
    "description": "usage: multi_process.py [-h] [--input_folder INPUT_FOLDER]",
    "args": [
      {
        "name": "input_folder",
        "help": ""
      },
      {
        "name": "output_folder",
        "help": ""
      },
      {
        "name": "scale",
        "help": "Scale factor for resizing"
      },
      {
        "name": "rotate",
        "help": "Rotation angle"
      }
    ]
  }
}